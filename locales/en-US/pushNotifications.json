{
  "CREATE_PARTY": {
    "title": "{{name}} created a {{tier}} {{defender}} raid party",
    "subtitle": "Raid Party #{{raidPartyId}}",
    "line1": "$t(pushNotifications:defaultBody.line1)",
    "line2": "$t(pushNotifications:defaultBody.line2)",
    "line3": "$t(pushNotifications:defaultBody.line3)",
    "line4": "$t(pushNotifications:defaultBody.line4)"
  },
  "ADD_USER": {
    "title": "{{name}} joined a {{tier}} {{defender}} raid party",
    "subtitle": "Raid Party #{{raidPartyId}}",
    "line1": "$t(pushNotifications:defaultBody.line1)",
    "line2": "$t(pushNotifications:defaultBody.line2)",
    "line3": "$t(pushNotifications:defaultBody.line3)",
    "line4": "$t(pushNotifications:defaultBody.line4)"
  },
  "REMOVE_USER": {
    "title": "{{name}} left a {{tier}} {{defender}} raid party",
    "subtitle": "Raid Party #{{raidPartyId}}",
    "line1": "$t(pushNotifications:defaultBody.line1)",
    "line2": "$t(pushNotifications:defaultBody.line2)",
    "line3": "$t(pushNotifications:defaultBody.line3)",
    "line4": "$t(pushNotifications:defaultBody.line4)"
  },
  "UPDATE_USER": {
    "title": "{{name}} changed their settings for {{tier}} {{defender}}",
    "subtitle": "Raid Party #{{raidPartyId}}",
    "line1": "$t(pushNotifications:defaultBody.line1)",
    "line2": "$t(pushNotifications:defaultBody.line2)",
    "line3": "$t(pushNotifications:defaultBody.line3)",
    "line4": "$t(pushNotifications:defaultBody.line4)"
  },
  "UPDATE_PARTY": {
    "title": "{{name}} updated raid settings for {{tier}} {{defender}}",
    "subtitle": "Raid Party #{{raidPartyId}}",
    "line1": "$t(pushNotifications:defaultBody.line1)",
    "line2": "$t(pushNotifications:defaultBody.line2)",
    "line3": "$t(pushNotifications:defaultBody.line3)",
    "line4": "$t(pushNotifications:defaultBody.line4)"
  },
  "defaultBody": {
    "line1": "{{numPlayers}} Players",
    "line2": "{{winPercent}}% Chance to Win",
    "line3": "{{powerPercent}}% Group Damage",
    "line4": "{{estimator}} Players Needed"
  },
  "discordLine": "#{{discordChannel}} - {{discordServerName}}",
  "globalRaidNetwork": {
    "avatar": "https://static.pokebattler.com/images/pokebattler_raid_party_logo.png",
    "username": "Pokebattler Raid Network",
    "title": "{{discordFlag}}**{{defender}}** - {{tier}} ({{discordLanguage}})",
    "description": "{{remoteSlotsEmoji}} players needed for {{tier}} **{{defender}}** raid - #{{raidPartyId}}\n\n>>> Join using the [Pokebattler Raid Party App](https://www.pokebattler.com/raidParty/{{raidPartyId}}) or by joining the Discord server and interacting with the {{botName}} Discord Raid Link.\n",
    "url": "https://www.pokebattler.com/raidParty/{{raidPartyId}}",
    "field1": {
      "name": "Join Discord Server",
      "value": "[{{discordGuildName}}]({{discordGuildInviteUrl}})"
    },
    "field2": {
      "name": "Discord Raid Link",
      "value": "[via {{botName}}](https://discord.com/channels/{{discordGuildId}}/{{discordChannelId}}/{{discordMessageId}})"
    },
    "field3": {
      "name": "One Click Join",
      "value": "[via Pokebattler Raid Network](https://www.pokebattler.com/raidParty/{{raidPartyId}}/join)"
    },
    "field1NonDiscord": {
      "name": "Only available in app",
      "value": "[Pokebattler Raid Party App](https://www.pokebattler.com/raidParty/{{raidPartyId}}/discord)"
    },
    "thumbnail": "{{pokemonIcon}}",
    "author": {
      "name": "{{botName}}",
      "icon_url": "{{botIcon}}"
    },
    "footer": "$t(pushNotifications:defaultBody.line1) - $t(pushNotifications:defaultBody.line2) - $t(pushNotifications:defaultBody.line3) - $t(pushNotifications:defaultBody.line4)"
  }
}